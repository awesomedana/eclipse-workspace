package dao;

import java.util.HashMap;
import java.util.List;

public interface PlayerMapper {
	public int insertP(HashMap<String, String> map);
	
	public int updateP(HashMap<String, String> map);
	
	public int deleteP(int p_no);
	
	public HashMap<String, Object> findByNo(int p_no);
	
	public List<HashMap<String, String>> findAll();
	
	public HashMap<String, String> findPTName(int p_no);
}
